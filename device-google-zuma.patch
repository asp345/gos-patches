+#!/vendor/bin/sh
+
+MODEM_DEVICE="/dev/umts_router"
+
+send_at_cmd() {
+  local command="$1"
+  log -t VoLTE-Config "SEND: $command"
+  echo -e "$command\r" > "$MODEM_DEVICE"
+  sleep 0.1
+}
+
+apply_common_settings() {
